          <script>

function showAnswers() {
  var inputs = document.getElementsByTagName('input');
  var answers = [];
  for (var i = 0; i < inputs.length; i++) {
    if (inputs[i].type == 'radio' && inputs[i].checked) {
      answers.push(inputs[i].value);
    }
  }

  // Hide the form
  document.getElementById('quiz-form').style.display = 'none';

  // Display the answers
  var answersDisplay = document.getElementById('answers-display');
  answersDisplay.style.display = 'block';
  
  var answerList = "<ul>";
  for (var j = 0; j < answers.length; j++) {
    answerList += "<li>" + answers[j] + "</li>";
  }
  answerList += "</ul>";
  answersDisplay.innerHTML = 'Jūsų maršruto objektai: ' + answerList;
}


          </script>

	
	</form>

	<div id="answers-display" style="display: none;"></div>

	<button type="button" onclick="showAnswers()">Ačiū, gana</button>

	
